{
  "name": "ai-workflow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "eslint . --max-warnings=0",
    "start": "next start",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "setup": "pnpm install && node -v && pnpm -v",
    "doctor": "pnpm lint && pnpm typecheck && pnpm test:run",
    "env:check": "node scripts/env-check.js",
    "clean": "node scripts/clean.js",
    "clean:deps": "pnpm dlx rimraf node_modules && rm -f pnpm-lock.yaml && pnpm install",
    "clean:logs": "pnpm dlx rimraf logs",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "deps:unused": "pnpm dlx depcheck",
    "verify": "pnpm lint && pnpm typecheck && pnpm test:run",
    "build:ci": "pnpm verify && NODE_ENV=production next build",
    "start:prod": "next start -p 3000",
    "docs:generate": "node scripts/generate-docs.js",
    "prepare": "husky install"
  },
