{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://agenticfabric.example/schema/executor/magentic-orchestrator/v1",
  "allOf": [
    { "$ref": "https://agenticfabric.example/schema/executor/base-props/v1" },
    {
      "type": "object",
      "properties": {
        "type": { "const": "magentic-orchestrator-executor" },
        "planningStrategy": { "type": "string", "enum": ["sequential", "parallel", "adaptive"] },
        "progressTracking": { "type": "boolean" },
        "humanInTheLoop": { "type": "boolean" }
      },
      "required": ["type"]
    }
  ]
}
