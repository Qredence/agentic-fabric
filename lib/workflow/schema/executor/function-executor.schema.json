{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://agenticfabric.example/schema/executor/function/v1",
  "allOf": [
    { "$ref": "https://agenticfabric.example/schema/executor/base-props/v1" },
    {
      "type": "object",
      "properties": {
        "type": { "const": "function-executor" },
        "functionName": { "type": "string" },
        "functionCode": { "type": "string" },
        "functionId": { "type": "string" },
        "parameters": { "type": "object" }
      },
      "required": ["type", "functionName"]
    }
  ]
}
